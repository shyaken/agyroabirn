Func getkeycode($phim)
	Select
		Case $phim = "F1"
			$phim = "0x70"
		Case $phim = "F2"
			$phim = "0x71"
		Case $phim = "F3"
			$phim = "0x72"
		Case $phim = "F4"
			$phim = "0x73"
		Case $phim = "F5"
			$phim = "0x74"
		Case $phim = "F6"
			$phim = "0x75"
		Case $phim = "F7"
			$phim = "0x76"
		Case $phim = "F8"
			$phim = "0x77"
	EndSelect
	Return $phim
EndFunc

Func readMemory ($name, $chandle, $type)
	Return _memoryread($memoryAddrArray($name), $chandle, $type)
EndFunc

Func readMemoryNoType ($name, $chandle)
	Return _memoryread($memoryAddrArray($name), $chandle)
EndFunc

Func writeMemory ($name, $chandle, $value)
	Return _memorywrite($memoryAddrArray($name), $chandle, $value)
EndFunc

Func getServerName ($text)
	$text = StringReplace($text, "@8Phuòng SôÌ(HOT)@1", "Phung So")
	$text = StringReplace($text, "@7Ngoaò Long(HOT)@1", "Ngoa Long")
	$text = StringReplace($text, "@7Chu Du (New)@1", "Chu Du")
	$text = StringReplace($text, "@8Tu Ma Y (NEW)@1", "Tu Ma Y")
	Return $text
EndFunc

Func getCurrentHp ()
	$addhpnomal = $memoryAddrArray($hpaddkey)
	$hp = _memoryread($addhpnomal, $handle)
	$hp = _memoryread($hp, $handle) + 8
	$hp = _memoryread($hp, $handle) + 36
	$hp = _memoryread($hp, $handle) + 56
	$hp = _memoryread($hp, $handle) + 0
	Return $hp
EndFunc

Func calLParam($x, $y)
	return ($y * 65536) + $x
EndFunc

Func getX ()
	Return Floor(readMemoryNoType($xkey,$handle)/64)
EndFunc

Func getY ()
	Return Floor(readMemoryNoType($ykey,$handle)/64)
EndFunc

Func getMouseX ($lastMouseX)
	Switch ($lastMouseX)
		Case 523
			Return 626
		Case 626
			Return 726
		Case 726
			Return 826
		Case 826
			Return 926
		Case 926
			Return 426
		Case 426
			Return 326
		Case 326
			Return 226
		Case 226
			Return 126
		Case Else
			Return 523
	EndSwitch
EndFunc

